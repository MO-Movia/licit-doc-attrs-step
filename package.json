{
  "name": "@modusoperandi/licit-doc-attrs-step",
  "version": "0.0.2",
  "subversion": "1",
  "description": "Module that extend Step, overriding the apply, invert, map, getMap and fromJSON methods, and registering your class with a unique JSON-serialization identifier using Step.jsonID.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MO-Movia/licit-doc-attrs-step.git"
  },
  "scripts": {
    "build:clean": "rm -rf dist/ && rm -f modusoperandi-licit-doc-attrs-step-*.*.*.tgz",
    "build:babel": "babel src --out-dir dist",
    "build:flow": "flow-copy-source -v -i '**/__tests__/**' src dist",
    "build:dist": "npm run build:clean && npm run build:babel && npm run build:flow",
    "flow": "flow --show-all-errors",
    "lint": "eslint \"src/**/*.js\"",
    "prepare": "npm run build:dist",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@babel/plugin-proposal-do-expressions": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-proposal-function-sent": "^7.10.4",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-proposal-pipeline-operator": "^7.10.4",
    "@babel/plugin-proposal-throw-expressions": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-flow-strip-types": "^7.10.4",
    "@babel/plugin-transform-parameters": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-flow": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "8.1.0",
    "babel-plugin-flow-react-proptypes": "26.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "eslint": "7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.20.6",
    "flow-bin": "0.128.0",
    "flow-copy-source": "2.0.9",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-prosemirror": "^0.8.3",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0"
  },
  "dependencies": {
    "prosemirror-model": "^1.13.1",
    "prosemirror-transform": "^1.2.9",
    "prosemirror-state": "^1.3.4",
    "prosemirror-view": "^1.17.3",
    "prosemirror-keymap": "^1.1.4",
    "browserkeymap": "2.0.2"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module-grouping"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!(*test|*.setup).js": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "jest": {
    "rootDir": "src",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "../jest.setup.js"
    ]
  }
}
