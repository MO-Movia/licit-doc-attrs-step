{
  "name": "@modusoperandi/licit-doc-attrs-step",
  "version": "0.1.6",
  "type": "module",
  "subversion": "1",
  "description": "Module that extend Step, overriding the apply, invert, map, getMap and fromJSON methods, and registering your class with a unique JSON-serialization identifier using Step.jsonID.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MO-Movia/licit-doc-attrs-step.git"
  },
  "scripts": {
    "build:clean": "rm -rf dist/ && rm -f modusoperandi-licit-doc-attrs-step-*.*.*.tgz",
    "build:ts": "tsc -p tsconfig.json",
    "build:babel": "babel src --out-dir dist --extensions .ts",
    "build:dist": "npm run build:clean && npm run build:ts && npm run webpack && npm run build:babel",
    "webpack": "webpack",
    "lint": "eslint src --ext .ts",
    "prepare": "npm run build:dist",
    "test": "jest --coverage",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "ci:build": "npm run build:clean && npm run build:ts && npm run webpack && npm run build:babel",
    "ci:bom": "cyclonedx-npm --ignore-npm-errors --short-PURLs --output-format XML --output-file dist/bom.xml",
    "debug": "node --debug-brk --inspect ./node_modules/.bin/jest -i"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@babel/plugin-proposal-do-expressions": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-proposal-function-sent": "^7.10.4",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-proposal-pipeline-operator": "^7.10.4",
    "@babel/plugin-proposal-throw-expressions": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-parameters": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.15.0",
    "@babel/plugin-proposal-json-strings": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.15.0",
    "@babel/plugin-proposal-numeric-separator": "^7.15.0",
    "@babel/plugin-proposal-optional-chaining": "^7.15.0",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.15.0",
    "@cyclonedx/cyclonedx-npm": "^1.11.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "babel-jest": "^29.2.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "8.25.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^27.1.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.0.3",
    "jest-junit": "^15.0.0",
    "jest-prosemirror": "^2.0.4",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^11.1.1",
    "prettier": "^2.1.2",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^22.0.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.2.0",
    "webpack": "^5.48.0",
    "webpack-cli": "^4.7.0",
    "write-file-webpack-plugin": "4.5.1"
  },
  "dependencies": {
    "browserkeymap": "2.0.2",
    "prosemirror-keymap": "^1.2.0",
    "prosemirror-model": "^1.16.0",
    "prosemirror-state": "^1.4.2",
    "prosemirror-transform": "^1.7.0",
    "prosemirror-view": "^1.27.0",
    "typescript": "^4.8.4"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module-grouping"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!(*test|*.setup).ts": [
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
